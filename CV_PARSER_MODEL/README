# Save JSON only, no output
python3 main.py parse_cv --path ~/Downloads/CV_Yasser_Jamli.pdf --save

# Save JSON and log to file
python3 main.py parse_cv --path ~/Downloads/CV_Yasser_Jamli.pdf --save --logging

# Save JSON and show console output
python3 main.py parse_cv --path ~/Downloads/CV_Yasser_Jamli.pdf --save --console

# Full output (save JSON, log to file, show console)
python3 main.py parse_cv --path ~/Downloads/CV_Yasser_Jamli.pdf --save --logging --console